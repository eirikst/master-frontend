<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".application.writer.profile.fragments.MyProfileHeader">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        >

        <com.andreasogeirik.master_frontend.layout.view.CustomTextView
            android:id="@+id/friend_profile_friends"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0 venner"
            android:layout_margin="10dp"
            />

        <com.andreasogeirik.master_frontend.layout.view.CustomTextView
            android:id="@+id/friend_profile_your_friend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Din venn"
            android:layout_below="@id/friend_profile_friends"
            android:layout_margin="10dp"
            />

    </RelativeLayout>
</FrameLayout>
